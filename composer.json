{
    "name": "notadd/composer",
    "keywords": ["notadd", "expand", "composer"],
    "homepage": "https://notadd.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Notadd",
            "email": "notadd@ibenchu.com"
        }
    ],
    "type": "notadd-extension",
    "autoload": {
        "psr-4": {
            "Notadd\\Composer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notadd\\Composer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "replace": {
        "psr/log": "1.0.*",
        "symfony/console": "3.3.*",
        "symfony/debug": "3.3.*",
        "symfony/finder": "3.3.*",
        "symfony/polyfill-mbstring": "3.3.*",
        "symfony/process": "3.3.*"
    },
    "require": {
        "php": ">=7.0",
        "composer/composer": "^1.5"
    },
    "require-dev": {
        "notadd/installers": "0.9.*",
        "phpunit/phpunit": "~6.0"
    }
}
